    "server:prod": "node dist/bin/harvest-mate.js",
    "typeorm": "typeorm-ts-node-esm",
    "migration:revert": "node dist/bin/migrations.js -a migrations:revert",
    "migration:run": "node dist/bin/migrations.js -a migrations:run",
    "codegen": "ts-node src/bin/code-generation.ts",
    "test": "jest --runInBand --config ./jest.config.ts --rootDir . ",
    "test:unit": "jest --config jest.config.unit.ts --passWithNoTests",
    "test:integration": "jest --runInBand --config jest.config.integration.ts",
    "seed": "ts-node src/bin/seeding.ts"
  },
